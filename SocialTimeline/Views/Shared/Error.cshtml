@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<h1 class="text-danger">Error :(</h1>
<h2 class="text-danger">Er is iets fout gegaan!</h2>
<br />
@if (!string.IsNullOrEmpty(ViewBag.Error))
{
    <p>
        <h2>Gedetaileerde weergave error:</h2>
        <br />
        @ViewBag.Error
    </p>
}